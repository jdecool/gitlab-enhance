{
  "name": "Gitlab Enhance",
  "description": "Improve Gitlab UI",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "224": "images/logo-224.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.gitlab.com/*",
        "*://gitlab.activinnov.net/*"
      ],
      "js": ["contentscript.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "gitlab-enhance@jdecool.fr",
      "strict_min_version": "42.0"
    }
  }
}
